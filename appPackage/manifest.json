{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.15/MicrosoftTeams.schema.json",
    "manifestVersion": "1.15",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Teams App, Inc.",
        "websiteUrl": "${{TAB_ENDPOINT}}",
        "privacyUrl": "${{TAB_ENDPOINT}}/privacy",
        "termsOfUseUrl": "${{TAB_ENDPOINT}}/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "Pazysalvos-Pgnv3-${{TEAMSFX_ENV}}",
        "full": "Full name for Pazysalvos-Pgnv3"
    },
    "description": {
        "short": "Short description of Pazysalvos-Pgnv3",
        "full": "Full description of Pazysalvos-Pgnv3"
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [
        {
            "entityId": "index",
            "name": "Inicio",
            "contentUrl": "${{TAB_ENDPOINT}}/",
            "websiteUrl": "${{TAB_ENDPOINT}}/",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index1",
            "name": "Areas",
            "contentUrl": "${{TAB_ENDPOINT}}#/areas",
            "websiteUrl": "${{TAB_ENDPOINT}}#/areas",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index2",
            "name": "Solicitudes",
            "contentUrl": "${{TAB_ENDPOINT}}#/solicitud",
            "websiteUrl": "${{TAB_ENDPOINT}}#/solicitud",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index3",
            "name": "Crea solicitud",
            "contentUrl": "${{TAB_ENDPOINT}}#/misdatos",
            "websiteUrl": "${{TAB_ENDPOINT}}#/misdatos",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index4",
            "name": "Parametrización",
            "contentUrl": "${{TAB_ENDPOINT}}/parametrizar",
            "websiteUrl": "${{TAB_ENDPOINT}}/parametrizar",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index5",
            "name": "Informes",
            "contentUrl": "${{TAB_ENDPOINT}}/informes",
            "websiteUrl": "${{TAB_ENDPOINT}}/informes",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index6",
            "name": "Historial",
            "contentUrl": "${{TAB_ENDPOINT}}/historial",
            "websiteUrl": "${{TAB_ENDPOINT}}/historial",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index7",
            "name": "Gestión documental",
            "contentUrl": "${{TAB_ENDPOINT}}/gestion",
            "websiteUrl": "${{TAB_ENDPOINT}}/gestion",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_DOMAIN}}"
    ],
    "webApplicationInfo": {
        "id": "${{AAD_APP_CLIENT_ID}}",
        "resource": "api://${{TAB_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
    }
}